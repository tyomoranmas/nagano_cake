<div class="row">
<div class="col-xs-1"></div>
<div class="col-xs-11">

 <div class="menber-title">
  <h1>会員詳細画面</h1>
 </div>

 <div class="menber-box">
  　　<label for="id"> 顧客ID </label>　　　　　　　　<%= @end_user.id %>
   <button class="btn btn-info btn-xs">
    <%= @end_user.is_deleted %>
   </button><br>
  　　<label for="name"> 氏名 </label>　　　　　　　　　<%= @end_user.last_name %>&nbsp;<%= @end_user.first_name %><br>
  　　<label for="name_kana"> フリガナ </label>　　　　　　　<%= @end_user.last_name_kana %>&nbsp;<%= @end_user.first_name_kana %><br>
  　　<label for="post_code"> 郵便番号 </label>　　　　　　　〒<%= @end_user.post_code %><br>
  　　<label for="address"> 住所 </label>　　　　　　　　　<%= @end_user.address %><br>
  　　<label for="phone_number"> 電話番号 </label>　　　　　　　<%= @end_user.phone_number %><br>
  　　<label for="email"> メールアドレス </label>　　　　<%= @end_user.email %><br>
 </div>

 <div class="actions">
　　<%= link_to "編集する", "/admins/end_users/#{@end_user.id}/edit", class: "btn btn-primary" %>
    <%= link_to "注文履歴一覧を見る", admins_orders_path, class: "btn btn-primary" %>
 </div>

</div>
</div>



