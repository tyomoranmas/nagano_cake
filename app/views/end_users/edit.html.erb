<h1>登録情報編集</h1>
<%= render 'layouts/error_message', model: @end_user %>
<div class="row">
  <div class="col-xs-12">
    <%= form_for @end_user do |f| %>
    <%= render 'end_users/shared/form', f: f %>
  </div>
  <div class="actions">
    <%= f.submit "編集内容を保存する", class: "btn btn-primary col-xs-3 col-xs-offset-1" %>
  </div>
  <% end %>
  <%= link_to "退会する", leave_end_user_path, class: "btn btn-danger col-xs-2 col-xs-offset-1" %>
</div>