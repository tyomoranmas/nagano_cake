<h3>登録情報編集</h3>

<div class="row">
  <div class="col-xs-12">
    <%= form_for @end_user do |f| %>
    <%= render 'end_users/shared/form', f: f %>
  </div>
  <div class="actions">
    <%= f.submit "編集内容を保存する", class: "btn btn-primary col-xs-3 col-xs-offset-1" %>
  </div>
  <% end %>
  <div class="btn btn-danger col-xs-2 col-xs-offset-1">
    <%= link_to "退会する" %>
  </div>
</div>